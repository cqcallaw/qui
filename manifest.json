{
	"name": "Qui",
	"version": "1.0",
	"description": "Trust management for distributed websites",
	"permissions": [
		"tabs",
		"declarativeContent",
		"storage",
		"notifications",
		"*://*.ipfs.localhost/*",
		"*://*.ipns.localhost/*"
	],
	"background": {
		"scripts": [
			"openpgp.js",
			"util.js",
			"main.js"
		],
		"persistent": false
	},
	"options_page": "options.html",
	"page_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "images/default16.png",
		"32": "images/default32.png",
		"48": "images/default48.png",
		"128": "images/default128.png"
	},
	"manifest_version": 2
}